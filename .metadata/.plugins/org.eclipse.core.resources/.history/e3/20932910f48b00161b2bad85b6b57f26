package solutions;

import java.util.ArrayList;

import basicTools.NumberCharacteristics;

public class Euler92 {

	
	
	public static void main(String[] args) {
		
		ArrayList<Long> digits;
		Long seqValue;
		int total = 0;
		
		for (Long i = new Long("1"); i <= 10; i++) {
			
			seqValue = i;
			
			while (seqValue != 1) {
				digits = NumberCharacteristics.getDigits(i);
				seqValue = digits.stream().map(x -> Math.pow(x, 2)).count();
				if (seqValue == 89) {
					total++;
					break;
				}
			}
			
			
		}
		
		System.out.println(total);
	}
	
}
